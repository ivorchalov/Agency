@model StrAgent.Models.StrSl

@{
    ViewData["Title"] = "EditStrSl";
    Layout = "_LayoutAgent";
}

<h1>EditStrSl</h1>

<h4>StrSl</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditStrSl">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <select asp-for="Status" class="form-control">
                    @foreach (var id in ViewBag.Statuses.Items)
                    {
                        <option value="@id">@id</option>
                    }
                </select>
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="hidden" asp-for="IDKl" class="form-control" />
                <span asp-validation-for="IDKl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="hidden" asp-for="IDAg" class="form-control" />
                <span asp-validation-for="IDAg" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IDDogv" class="control-label"></label>
                <label>@Model.IDDogv</label>
                <span asp-validation-for="IDDogv" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Date" class="control-label"></label>
                <input asp-for="Date" class="form-control" />
                <span asp-validation-for="Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Described" class="control-label"></label>
                <input asp-for="Described" class="form-control" />
                <span asp-validation-for="Described" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="StrSlList">Back to List</a>
</div>

